{
  "servers": {
    "github": {
      "url": "https://api.githubcopilot.com/mcp/",
      "type": "http"
    },
    "rust-language-server": {
      "command": "${userHome}/go/bin/mcp-language-server",
      "args": [
        "--workspace",
        "${workspaceFolder}",
        "--lsp",
        "rust-analyzer"
      ],
      "env": {
        "LOG_LEVEL": "DEBUG"
      }
    }
  }
}
